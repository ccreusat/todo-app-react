{
  "ci": {
    "collect": {
      "url": ["http://localhost:8000/"],
      "startServerCommand": "rails server -e production",
      "staticDistDir": "./dist"
    },
    "assert": {
      "assertions": {
        "categories:performance": "off",
        "categories:pwa": "off",
        "categories:accessibility": ["error", { "minScore": 1 }]
      }
    }
  }
}
